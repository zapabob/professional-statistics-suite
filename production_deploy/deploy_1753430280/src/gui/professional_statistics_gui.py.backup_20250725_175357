    def create_advanced_widgets(self):
        """é«˜åº¦ãªGUIã‚¦ã‚£ã‚¸ã‚§ãƒƒãƒˆã®ä½œæˆ"""
        # ãƒ¡ã‚¤ãƒ³ãƒ•ãƒ¬ãƒ¼ãƒ 
        main_frame = ttk.Frame(self.root)
        main_frame.pack(fill=tk.BOTH, expand=True, padx=10, pady=10)
        
        # ãƒãƒ¼ãƒˆãƒ–ãƒƒã‚¯ï¼ˆã‚¿ãƒ–ï¼‰ã®ä½œæˆ
        self.notebook = ttk.Notebook(main_frame)
        self.notebook.pack(fill=tk.BOTH, expand=True)
        
        # å„ã‚¿ãƒ–ã®ä½œæˆ
        self.create_data_management_tab()
        self.create_ai_analysis_tab()
        self.create_advanced_statistics_tab()
        self.create_bayesian_analysis_tab()
        self.create_survival_analysis_tab()
        self.create_power_analysis_tab()
        self.create_advanced_visualization_tab()
        self.create_assumption_validation_tab()
        self.create_machine_learning_tab()
        self.create_reports_tab()
        self.create_audit_compliance_tab()
        self.create_logs_tab()

    def create_data_management_tab(self):
        """ãƒ‡ãƒ¼ã‚¿ç®¡ç†ã‚¿ãƒ–"""
        data_frame = ttk.Frame(self.notebook)
        self.notebook.add(data_frame, text="ğŸ“Š ãƒ‡ãƒ¼ã‚¿ç®¡ç†")
        
        # ãƒ‡ãƒ¼ã‚¿æ“ä½œãƒ•ãƒ¬ãƒ¼ãƒ 
        data_ops_frame = ttk.LabelFrame(data_frame, text="ãƒ‡ãƒ¼ã‚¿æ“ä½œ", padding=10)
        data_ops_frame.pack(fill=tk.X, padx=5, pady=5)
        
        # ãƒœã‚¿ãƒ³ï¼ˆãƒ‡ãƒãƒƒã‚°ãƒ­ã‚°æ©Ÿèƒ½ä»˜ãï¼‰
        load_button = ttk.Button(data_ops_frame, text="CSVèª­ã¿è¾¼ã¿", 
                                command=self.create_debug_button_wrapper("CSVèª­ã¿è¾¼ã¿", self.load_csv_data))
        load_button.pack(side=tk.LEFT, padx=5)
        
        save_button = ttk.Button(data_ops_frame, text="ãƒ‡ãƒ¼ã‚¿ä¿å­˜", 
                                command=self.create_debug_button_wrapper("ãƒ‡ãƒ¼ã‚¿ä¿å­˜", self.save_data))
        save_button.pack(side=tk.LEFT, padx=5)
        
        clear_button = ttk.Button(data_ops_frame, text="ãƒ‡ãƒ¼ã‚¿ã‚¯ãƒªã‚¢", 
                                 command=self.create_debug_button_wrapper("ãƒ‡ãƒ¼ã‚¿ã‚¯ãƒªã‚¢", self.clear_data))
        clear_button.pack(side=tk.LEFT, padx=5)
        
        preprocess_button = ttk.Button(data_ops_frame, text="å‰å‡¦ç†å®Ÿè¡Œ", 
                                      command=self.create_debug_button_wrapper("å‰å‡¦ç†å®Ÿè¡Œ", self.run_data_preprocessing))
        preprocess_button.pack(side=tk.LEFT, padx=5)
        
        # ãƒ‡ãƒ¼ã‚¿è¡¨ç¤º
        data_display_frame = ttk.LabelFrame(data_frame, text="ãƒ‡ãƒ¼ã‚¿è¡¨ç¤º", padding=10)
        data_display_frame.pack(fill=tk.BOTH, expand=True, padx=5, pady=5)
        
        self.data_text = scrolledtext.ScrolledText(data_display_frame, height=20)
        self.data_text.pack(fill=tk.BOTH, expand=True)

    def create_ai_analysis_tab(self):
        """AIåˆ†æã‚¿ãƒ–ï¼ˆLLMåˆ‡ã‚Šæ›¿ãˆæ©Ÿèƒ½å¼·åŒ–ç‰ˆï¼‰"""
        ai_frame = ttk.Frame(self.notebook)
        self.notebook.add(ai_frame, text="ğŸ¤– AIåˆ†æ")
        
        # LLMãƒ—ãƒ­ãƒã‚¤ãƒ€ãƒ¼é¸æŠ
        provider_frame = ttk.LabelFrame(ai_frame, text="LLMãƒ—ãƒ­ãƒã‚¤ãƒ€ãƒ¼é¸æŠ", padding=10)
        provider_frame.pack(fill=tk.X, padx=5, pady=5)
        
        # ãƒ—ãƒ­ãƒã‚¤ãƒ€ãƒ¼é¸æŠã‚³ãƒ³ãƒœãƒœãƒƒã‚¯ã‚¹
        self.provider_var = tk.StringVar(value="auto")
        provider_combo = ttk.Combobox(provider_frame, textvariable=self.provider_var, 
                                     values=["auto", "google", "ollama", "lmstudio", "koboldcpp"], 
                                     state="readonly", width=15)
        provider_combo.pack(side=tk.LEFT, padx=5)
        ttk.Label(provider_frame, text="ãƒ—ãƒ­ãƒã‚¤ãƒ€ãƒ¼:").pack(side=tk.LEFT, padx=5)
        
        # ãƒ—ãƒ­ãƒã‚¤ãƒ€ãƒ¼çŠ¶æ…‹è¡¨ç¤º
        self.provider_status_label = ttk.Label(provider_frame, text="çŠ¶æ…‹: ç¢ºèªä¸­...")
        self.provider_status_label.pack(side=tk.RIGHT, padx=5)
        
        # ãƒ—ãƒ­ãƒã‚¤ãƒ€ãƒ¼çŠ¶æ…‹æ›´æ–°ãƒœã‚¿ãƒ³
        update_status_button = ttk.Button(provider_frame, text="çŠ¶æ…‹æ›´æ–°", 
                                         command=self.create_debug_button_wrapper("çŠ¶æ…‹æ›´æ–°", self.update_provider_status))
        update_status_button.pack(side=tk.RIGHT, padx=5)
        
        # GGUFãƒ¢ãƒ‡ãƒ«é¸æŠãƒœã‚¿ãƒ³
        gguf_button = ttk.Button(provider_frame, text="GGUFãƒ¢ãƒ‡ãƒ«é¸æŠ", 
                                 command=self.create_debug_button_wrapper("GGUFãƒ¢ãƒ‡ãƒ«é¸æŠ", self.select_gguf_model))
        gguf_button.pack(side=tk.RIGHT, padx=5)
        
        # ã‚¯ã‚¨ãƒªå…¥åŠ›
        query_frame = ttk.LabelFrame(ai_frame, text="è‡ªç„¶è¨€èªã‚¯ã‚¨ãƒª", padding=10)
        query_frame.pack(fill=tk.X, padx=5, pady=5)
        
        self.query_entry = ttk.Entry(query_frame, width=80)
        self.query_entry.pack(side=tk.LEFT, fill=tk.X, expand=True, padx=5)
        
        analyze_button = ttk.Button(query_frame, text="åˆ†æå®Ÿè¡Œ", 
                                   command=self.create_debug_button_wrapper("åˆ†æå®Ÿè¡Œ", self.execute_ai_analysis))
        analyze_button.pack(side=tk.RIGHT, padx=5)
        
        # çµæœè¡¨ç¤º
        result_frame = ttk.LabelFrame(ai_frame, text="AIåˆ†æçµæœ", padding=10)
        result_frame.pack(fill=tk.BOTH, expand=True, padx=5, pady=5)
        
        self.ai_result_text = scrolledtext.ScrolledText(result_frame, height=25)
        self.ai_result_text.pack(fill=tk.BOTH, expand=True)
        
        # åˆæœŸçŠ¶æ…‹æ›´æ–°
        self.update_provider_status()

    def create_advanced_statistics_tab(self):
        """é«˜åº¦çµ±è¨ˆåˆ†æã‚¿ãƒ–"""
        stats_frame = ttk.Frame(self.notebook)
        self.notebook.add(stats_frame, text="ğŸ“ˆ é«˜åº¦çµ±è¨ˆ")
        
        # åˆ†æã‚¿ã‚¤ãƒ—é¸æŠ
        analysis_frame = ttk.LabelFrame(stats_frame, text="åˆ†æã‚¿ã‚¤ãƒ—", padding=10)
        analysis_frame.pack(fill=tk.X, padx=5, pady=5)
        
        analysis_types = [
            "è¨˜è¿°çµ±è¨ˆ", "ç›¸é–¢åˆ†æ", "å›å¸°åˆ†æ", "åˆ†æ•£åˆ†æ", 
            "ã‚¯ãƒ©ã‚¹ã‚¿ãƒ¼åˆ†æ", "å› å­åˆ†æ", "æ™‚ç³»åˆ—åˆ†æ", "å¤šå¤‰é‡åˆ†æ"
        ]
        
        def create_analysis_button(analysis_type):
            return lambda: self.run_advanced_analysis(analysis_type)
        
        for i, analysis_type in enumerate(analysis_types):
            row = i // 4
            col = i % 4
            button = ttk.Button(analysis_frame, text=analysis_type, 
                               command=self.create_debug_button_wrapper(analysis_type, create_analysis_button(analysis_type)))
            button.grid(row=row, column=col, padx=5, pady=5)
        
        # çµæœè¡¨ç¤º
        result_frame = ttk.LabelFrame(stats_frame, text="åˆ†æçµæœ", padding=10)
        result_frame.pack(fill=tk.BOTH, expand=True, padx=5, pady=5)
        
        self.stats_result_text = scrolledtext.ScrolledText(result_frame, height=20)
        self.stats_result_text.pack(fill=tk.BOTH, expand=True)

    def create_bayesian_analysis_tab(self):
        """ãƒ™ã‚¤ã‚ºåˆ†æã‚¿ãƒ–"""
        bayes_frame = ttk.Frame(self.notebook)
        self.notebook.add(bayes_frame, text="ğŸ”® ãƒ™ã‚¤ã‚ºåˆ†æ")
        
        # ãƒ™ã‚¤ã‚ºåˆ†æã‚ªãƒ—ã‚·ãƒ§ãƒ³
        options_frame = ttk.LabelFrame(bayes_frame, text="ãƒ™ã‚¤ã‚ºåˆ†æã‚ªãƒ—ã‚·ãƒ§ãƒ³", padding=10)
        options_frame.pack(fill=tk.X, padx=5, pady=5)
        
