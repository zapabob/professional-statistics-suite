    # ãƒ­ã‚°ç®¡ç†ãƒ¡ã‚½ãƒƒãƒ‰
    def update_logs(self):
        """ãƒ­ã‚°ã®æ›´æ–°"""
        try:
            log_content = self.load_implementation_logs()
            if log_content:
                self.logs_text.delete(1.0, tk.END)
                self.logs_text.insert(tk.END, log_content)
            
            self.log_message("ãƒ­ã‚°ã‚’æ›´æ–°ã—ã¾ã—ãŸ")
            
        except Exception as e:
            messagebox.showerror("ã‚¨ãƒ©ãƒ¼", f"ãƒ­ã‚°ã®æ›´æ–°ã«å¤±æ•—ã—ã¾ã—ãŸ: {e}")
    
    def show_button_statistics(self):
        """ãƒœã‚¿ãƒ³çµ±è¨ˆã‚’è¡¨ç¤ºï¼ˆå¼·åŒ–ç‰ˆï¼‰"""
        try:
            if not hasattr(self, 'button_stats') or not self.button_stats:
                messagebox.showinfo("æƒ…å ±", "ã¾ã ãƒœã‚¿ãƒ³ãŒã‚¯ãƒªãƒƒã‚¯ã•ã‚Œã¦ã„ã¾ã›ã‚“")
                return
            
            # çµ±è¨ˆæƒ…å ±ã‚’åé›†
            total_clicks = sum(stats['clicks'] for stats in self.button_stats.values())
            total_success = sum(stats['success'] for stats in self.button_stats.values())
            total_failed = sum(stats['failed'] for stats in self.button_stats.values())
            overall_success_rate = (total_success / total_clicks * 100) if total_clicks > 0 else 0
            
            # çµ±è¨ˆãƒ†ã‚­ã‚¹ãƒˆã‚’ç”Ÿæˆ
            stats_text = "=== ãƒœã‚¿ãƒ³çµ±è¨ˆãƒ¬ãƒãƒ¼ãƒˆ ===\n"
            stats_text += f"ğŸ“Š ç·ã‚¯ãƒªãƒƒã‚¯å›æ•°: {total_clicks}\n"
            stats_text += f"âœ… ç·æˆåŠŸå›æ•°: {total_success}\n"
            stats_text += f"âŒ ç·å¤±æ•—å›æ•°: {total_failed}\n"
            stats_text += f"ğŸ“ˆ å…¨ä½“æˆåŠŸç‡: {overall_success_rate:.1f}%\n\n"
            stats_text += "=== å€‹åˆ¥ãƒœã‚¿ãƒ³çµ±è¨ˆ ===\n\n"
            
            # æˆåŠŸç‡ã§ã‚½ãƒ¼ãƒˆ
            sorted_stats = sorted(
                self.button_stats.items(),
                key=lambda x: (x[1]['success'] / x[1]['clicks'] if x[1]['clicks'] > 0 else 0),
                reverse=True
            )
            
            for button_name, stats in sorted_stats:
                success_rate = (stats['success'] / stats['clicks'] * 100) if stats['clicks'] > 0 else 0
                status_icon = "ğŸŸ¢" if success_rate >= 80 else "ğŸŸ¡" if success_rate >= 50 else "ğŸ”´"
                
                stats_text += f"{status_icon} {button_name}:\n"
                stats_text += f"   ã‚¯ãƒªãƒƒã‚¯å›æ•°: {stats['clicks']}\n"
                stats_text += f"   æˆåŠŸå›æ•°: {stats['success']}\n"
                stats_text += f"   å¤±æ•—å›æ•°: {stats['failed']}\n"
                stats_text += f"   æˆåŠŸç‡: {success_rate:.1f}%\n\n"
            
            # çµ±è¨ˆã‚’ãƒ­ã‚°ãƒ†ã‚­ã‚¹ãƒˆã«è¡¨ç¤º
            self.logs_text.delete(1.0, tk.END)
            self.logs_text.insert(tk.END, stats_text)
            
            self.log_message("ãƒœã‚¿ãƒ³çµ±è¨ˆãƒ¬ãƒãƒ¼ãƒˆã‚’è¡¨ç¤ºã—ã¾ã—ãŸ")
            
        except Exception as e:
            error_msg = f"ãƒœã‚¿ãƒ³çµ±è¨ˆã®è¡¨ç¤ºã«å¤±æ•—ã—ã¾ã—ãŸ: {e}"
            messagebox.showerror("ã‚¨ãƒ©ãƒ¼", error_msg)
            self.log_message(error_msg)

    def log_message(self, message):
        """ãƒ¡ãƒƒã‚»ãƒ¼ã‚¸ã®ãƒ­ã‚°è¨˜éŒ²"""
        timestamp = datetime.now().strftime("%Y-%m-%d %H:%M:%S")
        log_entry = f"[{timestamp}] {message}\n"
        print(log_entry.strip())
    
    def log_button_click(self, button_name: str, function_name: str, success: bool = True, error_msg: str = None):
        """ãƒœã‚¿ãƒ³ã‚¯ãƒªãƒƒã‚¯ãƒ­ã‚°ã‚’è¨˜éŒ²ï¼ˆå¼·åŒ–ç‰ˆï¼‰"""
        timestamp = datetime.now().strftime("%Y-%m-%d %H:%M:%S.%f")[:-3]
        status = "âœ… SUCCESS" if success else "âŒ FAILED"
        log_entry = f"[{timestamp}] ğŸ–±ï¸ BUTTON_CLICK: {button_name} -> {function_name} - {status}"
        
        if error_msg:
            log_entry += f" | ERROR: {error_msg}"
        
        # ã‚³ãƒ³ã‚½ãƒ¼ãƒ«ã«å‡ºåŠ›
        print(log_entry)
        
        # ãƒœã‚¿ãƒ³ãƒ‡ãƒãƒƒã‚°ãƒ­ã‚°ãƒ•ã‚¡ã‚¤ãƒ«ã«è¨˜éŒ²
        button_logger.info(log_entry)
        
        # GUIãƒ­ã‚°ã«ã‚‚è¨˜éŒ²
        if hasattr(self, 'logs_text'):
            self.logs_text.insert(tk.END, log_entry + "\n")
            self.logs_text.see(tk.END)
        
        # ãƒœã‚¿ãƒ³çµ±è¨ˆã‚’æ›´æ–°
        if not hasattr(self, 'button_stats'):
            self.button_stats = {}
        
        if button_name not in self.button_stats:
            self.button_stats[button_name] = {'clicks': 0, 'success': 0, 'failed': 0, 'last_click': None}
        
        self.button_stats[button_name]['clicks'] += 1
        self.button_stats[button_name]['last_click'] = timestamp
        
        if success:
            self.button_stats[button_name]['success'] += 1
        else:
            self.button_stats[button_name]['failed'] += 1
        
        # çµ±è¨ˆã®è‡ªå‹•ä¿å­˜
        self.save_button_statistics()
    
    def save_button_statistics(self):
        """ãƒœã‚¿ãƒ³çµ±è¨ˆã‚’è‡ªå‹•ä¿å­˜"""
        try:
            if hasattr(self, 'button_stats'):
                stats_file = os.path.join('logs', 'button_statistics.json')
                with open(stats_file, 'w', encoding='utf-8') as f:
                    json.dump(self.button_stats, f, ensure_ascii=False, indent=2)
        except Exception as e:
            print(f"ãƒœã‚¿ãƒ³çµ±è¨ˆä¿å­˜ã‚¨ãƒ©ãƒ¼: {e}")
    
    def load_button_statistics(self):
        """ãƒœã‚¿ãƒ³çµ±è¨ˆã‚’èª­ã¿è¾¼ã¿"""
        try:
            stats_file = os.path.join('logs', 'button_statistics.json')
            if os.path.exists(stats_file):
                with open(stats_file, 'r', encoding='utf-8') as f:
                    self.button_stats = json.load(f)
            else:
                self.button_stats = {}
        except Exception as e:
            print(f"ãƒœã‚¿ãƒ³çµ±è¨ˆèª­ã¿è¾¼ã¿ã‚¨ãƒ©ãƒ¼: {e}")
            self.button_stats = {}
    
    def create_unified_button_wrapper(self, button_name: str, original_function: Callable):
        """çµ±ä¸€ã•ã‚ŒãŸãƒœã‚¿ãƒ³ãƒ©ãƒƒãƒ‘ãƒ¼ï¼ˆãƒ‡ãƒãƒƒã‚°ãƒ­ã‚° + å¿œç­”æ€§æœ€é©åŒ–ï¼‰"""
        def unified_wrapper(*args, **kwargs):
            try:
                # ãƒœã‚¿ãƒ³ã‚¯ãƒªãƒƒã‚¯ãƒ­ã‚°ã‚’è¨˜éŒ²
                self.log_button_click(button_name, original_function.__name__, True)
                
                # å…ƒã®é–¢æ•°ã‚’å®Ÿè¡Œ
                result = original_function(*args, **kwargs)
                
                # æˆåŠŸãƒ­ã‚°ã‚’è¨˜éŒ²
                self.log_message(f"ãƒœã‚¿ãƒ³ '{button_name}' ãŒæ­£å¸¸ã«å®Ÿè¡Œã•ã‚Œã¾ã—ãŸ")
                
                return result
                
            except Exception as e:
                error_msg = str(e)
                # ã‚¨ãƒ©ãƒ¼ãƒ­ã‚°ã‚’è¨˜éŒ²
                self.log_button_click(button_name, original_function.__name__, False, error_msg)
                
                # ãƒ¦ãƒ¼ã‚¶ãƒ¼ãƒ•ãƒ¬ãƒ³ãƒ‰ãƒªãƒ¼ãªã‚¨ãƒ©ãƒ¼ãƒ¡ãƒƒã‚»ãƒ¼ã‚¸ã‚’è¡¨ç¤º
                error_display_msg = f"ãƒœã‚¿ãƒ³ '{button_name}' ã®å®Ÿè¡Œä¸­ã«ã‚¨ãƒ©ãƒ¼ãŒç™ºç”Ÿã—ã¾ã—ãŸ:\n{error_msg}"
                messagebox.showerror("ã‚¨ãƒ©ãƒ¼", error_display_msg)
                
                # ã‚³ãƒ³ã‚½ãƒ¼ãƒ«ã«ã‚‚è©³ç´°ã‚’å‡ºåŠ›
                print(f"è©³ç´°ã‚¨ãƒ©ãƒ¼æƒ…å ±: {traceback.format_exc()}")
                
                # ã‚¨ãƒ©ãƒ¼ã‚’å†ç™ºç”Ÿã•ã›ãªã„ï¼ˆGUIãŒãƒ•ãƒªãƒ¼ã‚ºã—ãªã„ã‚ˆã†ã«ï¼‰
                return None
        
        return unified_wrapper

    def create_debug_button_wrapper(self, button_name: str, original_function: Callable):
        """ãƒ‡ãƒãƒƒã‚°ç”¨ãƒœã‚¿ãƒ³ãƒ©ãƒƒãƒ‘ãƒ¼ï¼ˆå¾Œæ–¹äº’æ›æ€§ã®ãŸã‚ï¼‰"""
        return self.create_unified_button_wrapper(button_name, original_function)

    def optimize_button_responsiveness(self, button: tk.Widget, original_command: Callable):
        """ãƒœã‚¿ãƒ³å¿œç­”æ€§æœ€é©åŒ–ï¼ˆå¾Œæ–¹äº’æ›æ€§ã®ãŸã‚ï¼‰"""
        return self.create_unified_button_wrapper("å¿œç­”æ€§æœ€é©åŒ–", original_command)
    
    def get_responsiveness_report(self) -> Dict[str, Any]:
        """å¿œç­”æ€§ãƒ¬ãƒãƒ¼ãƒˆå–å¾—"""
        return self.responsiveness_optimizer.get_responsiveness_report()
    
    def run_responsiveness_test(self) -> Dict[str, Any]:
        """å¿œç­”æ€§ãƒ†ã‚¹ãƒˆå®Ÿè¡Œ"""
        return self.responsiveness_test_suite.run_comprehensive_test()

    def load_implementation_logs(self):
        """å®Ÿè£…ãƒ­ã‚°ã®èª­ã¿è¾¼ã¿"""
        try:
            log_file = "_docs/implementation_log_2025-01-27.md"
            if os.path.exists(log_file):
                with open(log_file, 'r', encoding='utf-8') as f:
                    log_content = f.read()
                return log_content
            else:
                return "å®Ÿè£…ãƒ­ã‚°ãƒ•ã‚¡ã‚¤ãƒ«ãŒè¦‹ã¤ã‹ã‚Šã¾ã›ã‚“"
        except Exception as e:
            return f"å®Ÿè£…ãƒ­ã‚°èª­ã¿è¾¼ã¿ã‚¨ãƒ©ãƒ¼: {e}"

    def setup_auto_save(self):
        """è‡ªå‹•ä¿å­˜ã®è¨­å®š"""
        def auto_save():
            try:
                if not self.data.empty:
                    backup_file = os.path.join(self.backup_dir, f"backup_{self.session_id}.csv")
                    self.data.to_csv(backup_file, index=False)
                    print(f"è‡ªå‹•ä¿å­˜å®Œäº†: {backup_file}")
            except Exception as e:
                print(f"è‡ªå‹•ä¿å­˜ã‚¨ãƒ©ãƒ¼: {e}")
            
            # 5åˆ†å¾Œã«å†å®Ÿè¡Œ
            self.root.after(300000, auto_save)
        
        # åˆå›å®Ÿè¡Œ
        self.root.after(300000, auto_save)

def main():
    """ãƒ¡ã‚¤ãƒ³é–¢æ•°"""
    root = tk.Tk()
    app = ProfessionalStatisticsGUI(root)
    
    def on_closing():
        """ã‚¢ãƒ—ãƒªã‚±ãƒ¼ã‚·ãƒ§ãƒ³çµ‚äº†æ™‚ã®å‡¦ç†"""
        try:
            # ã‚»ãƒƒã‚·ãƒ§ãƒ³çµ‚äº†å‡¦ç†
            print("Professional Statistics Suiteã‚’çµ‚äº†ã—ã¾ã™")
        except Exception as e:
            print(f"çµ‚äº†å‡¦ç†ã‚¨ãƒ©ãƒ¼: {e}")
        finally:
            root.destroy()
    
    root.protocol("WM_DELETE_WINDOW", on_closing)
    root.mainloop()

if __name__ == "__main__":
    main() 